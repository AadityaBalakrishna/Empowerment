#!/bin/bash

# Define API URL
URL="https://pcsvc.dt.pcapqa.app/quote-mr-1037/api/quote/getHistoricalQuotesAsOf"

# Define JSON Payload
DATA='{
  "securityIds": ["AAPL"],
  "userGuid": "fa4b5e15d2fa45e2910dcc02c4be3aa8",
  "fromDate": "2025-03-04",
  "toDate": "2025-03-05",
  "periods": 4,
  "periodType": "",
  "includeYOUHistory": true,
  "includePCBlended": true,
  "userAccountIdsString": "",
  "clientType": "",
  "currentDeviceProperties": "",
  "sessionId": "",
  "initiatedBy": "hukm",
  "priceDate": "2025-03-08",
  "quotesHistoryAsOf": true
}'

# Execute CURL command
curl -X POST "$URL" \
     -H "Content-Type: application/json" \
     -d "$DATA"

# Print message after execution
echo "API request sent!"
