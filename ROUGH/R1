3)VM configurations:

-DTEST_CONFIG_HOME=/home/abalakrishna/glab/pcap-server/pcap
-Dspring.main.allow-bean-definition-overriding=true
-Daws.account=dev
-Didentity.stage=devtrunk
-Didentity.parentStage=devshared
-Didentity.serverArray=pcap
-Dpcap.enableConfigStore=true
-Dpcsvc.server.useLocalEKSUrl=false

package com.personalcapital.config;

import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.Test;
import org.springframework.beans.factory.annotation.Value;
import org.springframework.test.context.junit.jupiter.SpringJUnitConfig;

@SpringJUnitConfig(locations = "classpath:pcapConfigApplicationContext.xml")
public class TestConfig
{
	@Value("${db.username}")
	private String prop;

	@Value("${pcap.ace.datasource.username}")
	private String user;

	@Value("${identity.serverArray}")
	private String configprop;

	@Test
	public void loadProperties()
	{
		Assertions.assertNotNull(prop);
		Assertions.assertEquals("pcap_app_2", prop);
	}

	@Test
	public void loadusername()
	{
		Assertions.assertNotNull(user);
		Assertions.assertEquals("root_app_devtrunk", user);
	}

	@Test
	public void loadConfigProperties()
	{
		Assertions.assertNotNull(configprop);
		Assertions.assertEquals("pcap", configprop);
	}
}
