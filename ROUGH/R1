<plugin>
  <groupId>org.classycle</groupId>
  <artifactId>classycle-maven-plugin</artifactId>
  <version>1.4.1</version>
  <executions>
    <execution>
      <goals>
        <goal>analyze</goal>
      </goals>
    </execution>
  </executions>
  <configuration>
    <reportFile>${project.build.directory}/classycle-report.txt</reportFile>
  </configuration>
</plugin>