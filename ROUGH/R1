package com.empower.epw.aws.api.kms.dto;

public class EncryptResponseDTO {
    private String ciphertext;

    // Default Constructor
    public EncryptResponseDTO() {
    }

    // Parameterized Constructor
    public EncryptResponseDTO(String ciphertext) {
        this.ciphertext = ciphertext;
    }

    // Getters and Setters
    public String getCiphertext() {
        return ciphertext;
    }
    public void setCiphertext(String ciphertext) {
        this.ciphertext = ciphertext;
    }
}

package com.empower.epw.aws.api.kms.dto;

public class EncryptRequestDTO {
    private String plaintext;
    private String keyId;

    // Default Constructor
    public EncryptRequestDTO() {
    }

    // Parameterized Constructor
    public EncryptRequestDTO(String plaintext, String keyId) {
        this.plaintext = plaintext;
        this.keyId = keyId;
    }

    // Getters and Setters
    public String getPlaintext() {
        return plaintext;
    }

    public void setPlaintext(String plaintext) {
        this.plaintext = plaintext;
    }

    public String getKeyId() {
        return keyId;
    }

    public void setKeyId(String keyId) {
        this.keyId = keyId;
    }
}

package com.empower.epw.aws.api.kms.dto;

public class DecryptResponseDTO {
    private String plaintext;

    // Default Constructor
    public DecryptResponseDTO() {
    }

    // Parameterized Constructor
    public DecryptResponseDTO(String plaintext) {
        this.plaintext = plaintext;
    }

    // Getters and Setters
    public String getPlaintext() {
        return plaintext;
    }

    public void setPlaintext(String plaintext) {
        this.plaintext = plaintext;
    }
}
package com.empower.epw.aws.api.kms.dto;

public class DecryptRequestDTO {
    private String ciphertext;

    // Default Constructor
    public DecryptRequestDTO() {
    }
    // Parameterized Constructor
    public DecryptRequestDTO(String ciphertext) {
        this.ciphertext = ciphertext;
    }
    // Getters and Setters
    public String getCiphertext() {
        return ciphertext;
    }
    public void setCiphertext(String ciphertext) {
        this.ciphertext = ciphertext;
    }
}


