# Enable export of metrics to statsd/Datadog
cache.metrics.enabled=false
management.defaults.metrics.export.enabled=true
management.endpoint.metrics.enabled=true
management.statsd.metrics.export.enabled=true
management.statsd.metrics.export.host=${HOST_IP}
management.statsd.metrics.export.flavor=datadog
management.metrics.tags.service=${DD_SERVICE}
management.metrics.tags.env=${DD_ENV}
management.metrics.tags.version=${DD_VERSION}

