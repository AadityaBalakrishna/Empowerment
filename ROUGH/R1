@Autowired
    private EpwConfigServiceBootstrapConfiguration configService;

    @Test
    public void testConfigClientPropertiesProfile() {
        // Retrieve the ConfigClientProperties bean
        ConfigClientProperties properties = configService.configClientProperties();

        // Get the profile set by inferProfiles()
        String profile = properties.getProfile();

        // Print the profile for debugging
        System.out.println("Inferred Profile: " + profile);

        // Assertions
        assertNotNull(profile, "Profile should not be null");
        assertFalse(profile.isBlank(), "Profile should not be blank");

        // Optional: Add assertions based on expected values
        // Example:
        // assertTrue(profile.contains("default-shared"));
    }
}
