  SqsCrmGatewayDryRun:
    Type: AWS::SQS::Queue
    UpdateReplacePolicy: Retain
    DeletionPolicy: Retain
    Properties:
      QueueName: !Join [ "_", [ !FindInMap [ StageMap, !Ref Stage, capName ], CRM_GATEWAY, DRY_RUN ] ]
      Tags:
        - { Key: "pc:silo", Value: pcap }
        - { Key: "pc:app", Value: pcapeks }
        - { Key: "pc:stage", Value: !Ref Stage }
        - { Key: "pc:service", Value: "crm-gateway" }
        # Datadog used tags
        - { Key: "dd-enabled", Value: true }
        - { Key: "dd-stage", Value: !Ref Stage }
        - { Key: "kube_service", Value: "crm-gateway-service" }
        - { Key: "queue_priority", Value: dry_run }
        - { Key: "GitPath", Value: !Ref GitPath }
        - { Key: "ManagedBy", Value: !Ref ManagedBy }
