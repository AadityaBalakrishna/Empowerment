	private static final String METRICS_NAME = "pcap.cache.event";

	void recordCaching(ProceedingJoinPoint pjp, String outcome){
		String className = getClassName(pjp);
		String methodName = getMethodName(pjp);

		Metrics.counter(METRICS_NAME,"class",className,"method",methodName,"outcome",outcome).increment();
	}

	private String getClassName(ProceedingJoinPoint pjp)
	{
		return pjp.getTarget()
				.getClass()
				.getSimpleName();
	}

	private String getMethodName(ProceedingJoinPoint pjp)
	{
		return pjp.getSignature()
				.getName();
	}


					/*// Cache Miss Metric
					Metrics.counter(METRICS_NAME, "class", className, "method", methodName, "outcome",
									"miss")
							.increment();
*/
					recordCaching(pjp,"miss");

there are the changes you need to keep in mind before giving me the test case
