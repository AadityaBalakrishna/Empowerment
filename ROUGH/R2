import org.mockito.Mock;
import org.mockito.Mockito;
import org.mockito.MockitoAnnotations;
import org.mockito.Spy;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.test.util.ReflectionTestUtils;

import java.util.List;
import java.util.UUID;

import jakarta.persistence.NoResultException;

import static org.hamcrest.CoreMatchers.is;
import static org.hamcrest.CoreMatchers.not;
import static org.hamcrest.CoreMatchers.nullValue;
import static org.hamcrest.MatcherAssert.assertThat;
import static org.mockito.Mockito.when;

public class UserPortfolioUserAccountMapServiceTest extends BaseDaoTest
{

	@Autowired
	private UserPortfolioUserAccountMapService userPortfolioUserAccountMapService;

	@Autowired
	private UserPortfolioUserAccountMapDao userPortfolioUserAccountMapDao;

	@Autowired
	private UserHouseholdPortfolioDao userHouseholdPortfolioDao;

	@Spy
	@Autowired
	private UpdateUserAccountHelper updateUserAccountHelper;

	@Autowired
	private UpdateUserAccountProcessor updateUserAccountProcessor;

	@Mock
	private UserAccountProcessService userAccountProcessService;

	private UserAccount userAccount;

	@BeforeEach
	public void setUpUserAccount() throws SafePageException
	{
		MockitoAnnotations.openMocks(this);
		ReflectionTestUtils.setField(updateUserAccountProcessor, "updateUserAccountHelper",
				updateUserAccountHelper);
		ReflectionTestUtils.setField(updateUserAccountHelper, "userAccountProcessService",
				userAccountProcessService);

		this.testUser = initializeUserOrCreateNew("user" + System.currentTimeMillis());

		userAccount = mock(UserAccount.class);
		long userAccountId = RandomUtils.nextLong(1, 10000);
		when(userAccount.getId()).thenReturn(userAccountId);
		UUID userAccountGuid = UUID.randomUUID();
		when(userAccount.getUserAccountGuid()).thenReturn(userAccountGuid);
		when(userAccount.getUser()).thenReturn(testUser);
		when(userAccount.getUserId()).thenReturn(testUser.getId());
		when(userAccount.isOnUs()).thenReturn(false);
		when(userAccount.getYodleeAccountId()).thenReturn("yodleeaccount");
		when(userAccount.getName()).thenReturn(testName);
		when(userAccountProcessService.getUserAccounts(testUser.getId()))
				.thenReturn(List.of(userAccount));
		when(userAccountProcessService.getUserAccount(testUser.getId(), userAccount.getId()))
				.thenReturn(userAccount);
	}

	@Test
	public void persist_whenValidEntity_persistEntity() throws Exception
	{
		UserPortfolioUserAccountMap userPortfolioUserAccountMap = persistUserPortfolioUserAccountMap();
		assertNotNull(userPortfolioUserAccountMap.getUserAccount());
		assertThat(userPortfolioUserAccountMap.getUserAccount()).isEqualTo(userAccount);
		assertNotNull(userPortfolioUserAccountMap.getUserAccount()
				.getUser());
		assertThat(userPortfolioUserAccountMap.getUserAccount()
				.getUser()).isEqualTo(testUser);
		verify(updateUserAccountHelper, Mockito.times(1)).update(userPortfolioUserAccountMap);
	}

	@Test
	public void findByUserAccountId_whenValidId_returnEntity() throws Exception
	{
		UserPortfolioUserAccountMap userPortfolioUserAccountMap = persistUserPortfolioUserAccountMap();

		UserPortfolioUserAccountMap actualUserPortfolioUserAccountMap = userPortfolioUserAccountMapService
				.findByUserAccountId(userPortfolioUserAccountMap.getUserAccountId());
		verify(updateUserAccountHelper, Mockito.times(2)).update(actualUserPortfolioUserAccountMap);
		assertThat(actualUserPortfolioUserAccountMap, is(not(nullValue())));
		assertThat(actualUserPortfolioUserAccountMap.getUserAccount(), is(userAccount));
		assertThat(actualUserPortfolioUserAccountMap.getUserAccount()
				.getUser(), is(testUser));
		assertThat(actualUserPortfolioUserAccountMap.getUserAccount()
				.getUser(), is(not(nullValue())));
		assertThat(actualUserPortfolioUserAccountMap.getUserAccountId(), is(userAccount.getId()));
		assertEquals(actualUserPortfolioUserAccountMap.getUserPortfolioGuid(),
				userPortfolioUserAccountMap.getUserPortfolioGuid());
		assertNotNull(actualUserPortfolioUserAccountMap.getUserPortfolioUserAccountMapGuid());
	}
}

error - Mockito Unable to initialize @Spy annotated field 'updateUserAccountHelper'.
