this is the failing test:

package com.empower.epw.userportfolio.service;

import com.empower.account.service.UserAccountProcessService;
import com.empower.epw.userportfolio.service.aspect.UpdateUserAccountProcessor;
import com.empower.epw.userportfolio.service.helper.UpdateUserAccountHelper;
import com.personalcapital.platform.persistence.common.utils.UuidUtils;
import com.personalcapital.user.data.UserHouseholdPortfolio;
import com.personalcapital.user.data.UserHouseholdPortfolioDao;
