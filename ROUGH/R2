docker run --rm -d -p 4566:4566 --name localstack localstack/localstack:3.0

# using awslocal
awslocal sqs create-queue --queue-name DEV_CRM_GATEWAY_NORMAL
awslocal sqs create-queue --queue-name DEV_CRM_GATEWAY_DRY_RUN
awslocal sqs create-queue --queue-name DEV_CRM_GATEWAY_INTERMEDIATE
awslocal sqs create-queue --queue-name DEV_CRM_GATEWAY_TOP

# or with aws cli:
aws --endpoint-url=http://localhost:4566 sqs create-queue --queue-name DEV_CRM_GATEWAY_NORMAL

aws --endpoint-url=http://localhost:4566 sqs list-queues
